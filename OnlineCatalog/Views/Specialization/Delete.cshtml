@model Specialization

<h3 class="my-5">
	@Model.Name
</h3>


<div class="col-md-8">

	<div class="col-5">
		<label asp-for="Name" class="form-label"></label>
		<input asp-for="Name" class="form-control" readonly />

		<form asp-action="Delete" class="my-3" method="post">
			<input type="submit" class="btn btn-danger" value="@Localizer["Confirm"]" />
		</form>

		<a class="btn btn-primary" asp-action="Index">@Localizer["Back"]</a>

	</div>



</div>